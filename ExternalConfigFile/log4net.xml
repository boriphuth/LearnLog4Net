<?xml version="1.0" encoding="utf-8" ?>
<log4net>
  <!--must be unique name-->
  <appender name="Console" type="log4net.Appender.ConsoleAppender">
    <layout type="log4net.Layout.SimpleLayout"/>
  </appender>

  <!--default for all logger-->
  <root>
    <level value="ALL"/>
    <appender-ref ref="Console"/>
  </root>

</log4net>
